<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Form view: -->
    <record id="knowledge_article_view_form" model="ir.ui.view">
        <field name="name">knowledge.article.view.form.inherit.diagram</field>
        <field name="model">knowledge.article</field>
        <field name="inherit_id" ref="knowledge.knowledge_article_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="/form" position="inside">
                <field name="show_load_diagram" invisible="1"/>
                <field name="diagram" invisible="1"/>
                <field name="diagram_version_id" invisible="1"/>
            </xpath>
        </field>
    </record>
</odoo>